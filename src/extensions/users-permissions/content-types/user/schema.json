{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "userPhoneNumber": {
      "type": "string"
    },
    "user_purchased_courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course-item.course-item",
      "mappedBy": "users"
    },
    "xxxxx": {
      "type": "string"
    },
    "favourite_items": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course-item.course-item",
      "inversedBy": "favourite_users"
    },
    "completed_course_videos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course-video-item.course-video-item",
      "inversedBy": "completed_course_users"
    },
    "userLearningStreak": {
      "type": "integer"
    },
    "lastCheckedDate": {
      "type": "date"
    },
    "creditCards": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::credit-card.credit-card",
      "inversedBy": "users_cards"
    },
    "message_notifications": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::message-notification.message-notification",
      "inversedBy": "users"
    }
  }
}
